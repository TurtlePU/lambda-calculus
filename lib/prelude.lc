true = \x y -> x
false = \x y -> y
not b = b false true

pair x y = \f -> f x y
first p = p (\x y -> x)
second p = p (\x y -> y)

zero = \s z -> z
one = \s z -> s z

isZ n = n (\a -> false) true

plus n m = \s z -> n s (m s z)
mul n m = n (plus m) zero

pred n = second (n (\p -> pair (plus (first p) (second p)) (first p)) (pair zero zero))
minus n m = m pred n

le n m = isZ (minus m n)
ge n m = le m n
lt n m = not (ge n m)
gt n m = not (le n m)

fix = (\f -> (\x -> f (x x))(\x -> f (x x)))